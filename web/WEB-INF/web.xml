<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientRegisterServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.PatientRegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDoctorServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.AddDoctorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddQuestionServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.AddQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAppointmentServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.AddAppointmentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPrescriptionServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.AddPrescriptionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveQuestionnaireServlet</servlet-name>
        <servlet-class>com.patient_support_system.servlets.SaveQuestionnaireServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientRegisterServlet</servlet-name>
        <url-pattern>/PatientRegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDoctorServlet</servlet-name>
        <url-pattern>/AddDoctorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddQuestionServlet</servlet-name>
        <url-pattern>/AddQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAppointmentServlet</servlet-name>
        <url-pattern>/AddAppointmentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPrescriptionServlet</servlet-name>
        <url-pattern>/AddPrescriptionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveQuestionnaireServlet</servlet-name>
        <url-pattern>/SaveQuestionnaireServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
